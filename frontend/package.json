{
  "name": "boot-react-frontend",
  "version": "0.0.1",
  "description": "Spring boot and react frontend",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "bundle": "cross-env NODE_ENV=production webpack -p",
    "test": "yarn run test:unit && yarn run test:e2e",
    "test:unit": "cross-env NODE_ENV=test mocha --require ./test/configureAdapter.js --recursive test/unit",
    "test:e2e": "cross-env NODE_ENV=test mocha ./test/setup.js --require ./test/configureAdapter.js --recursive test/endToEnd",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "William Lindner",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.6",
    "@storybook/addon-links": "^3.3.6",
    "@storybook/react": "^3.3.7",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.4.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "lodash": "^4.17.4",
    "mocha": "^4.1.0",
    "puppeteer": "^0.13.0",
    "react-addons-test-utils": "^15.0.0",
    "react-hot-loader": "^3.1.3",
    "react-router-dom": "^4.2.2",
    "storybook-router": "^0.3.2",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0"
  },
  "dependencies": {
    "apollo-client-preset": "^1.0.6",
    "deep-equal": "^1.0.1",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.3.1",
    "styled-components": "^2.4.0"
  }
}
